{
  "name": "@leverj/stake",
  "version": "0.0.13",
  "description": "Stake LEV tokens here to get FEE tokens for a particular staking-interval",
  "main": "index.js",
  "scripts": {
    "solium": "solium -d contracts/",
    "solhint": "solhint contracts/**/*.sol",
    "lint": "npm run solium & npm run solhint",
    "test": "BLUEBIRD_DEBUG=1 node $NODE_DEBUG_OPTION ./node_modules/.bin/truffle test",
    "bump:patch": "npm version patch && git push --follow-tags",
    "bump:minor": "npm version minor && git push --follow-tags",
    "bump:major": "npm version major && git push --follow-tags",
    "docker": "bin/publish.sh",
    "docker:local": "LOCAL=true bin/publish.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leverj/staking.git"
  },
  "keywords": [
    "leverj",
    "LEV",
    "FEE",
    "coinpit",
    "leverage",
    "trading"
  ],
  "author": "leverj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leverj/staking/issues"
  },
  "homepage": "https://github.com/leverj/staking#readme",
  "devDependencies": {
    "eth-gas-reporter": "^0.1.10",
    "ethjs-rpc": "^0.2.0",
    "expect.js": "^0.3.1",
    "solhint": "^1.2.1",
    "solium": "^1.1.8",
    "truffle": "^4.1.13",
    "truffle-flattener": "^1.2.5"
  },
  "dependencies": {
    "affirm.js": "^1.0.0",
    "bluebird": "^3.5.1",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "web3": "1.0.0-beta.35"
  }
}
